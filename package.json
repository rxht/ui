{
  "name": "rxh-ui",
  "license": "MIT",
  "version": "0.0.2",
  "author": "荣轩浩 <brf9577@163.com>",
  "description": "ui组件库",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts --host",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node script/copy.js",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "push": "npm run build:lib && npm publish",
    "new": "node ./script/new.js"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "vite",
    "component",
    "ui",
    "typescript",
    "vertical",
    "horizontal"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/rxhui.umd.js",
  "module": "./dist/rxhui.es.js",
  "types": "./dist/packages/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rxhui.es.js",
      "require": "./dist/rxhui.umd.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "lodash": "^4.17.21",
    "prismjs": "^1.28.0",
    "vite-plugin-md": "^0.13.1",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-prismjs": "^2.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-eslint": "^1.6.0",
    "vite-plugin-prismjs": "0.0.8",
    "vue-tsc": "^0.34.7"
  }
}
